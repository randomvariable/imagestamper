/dev/disk/by-partlabel/root / xfs defaults,x-systemd.device-timeout=0 0 0
/dev/disk/by-partlabel/boot /boot ext4 defaults 1 2
/dev/disk/by-partlabel/efi /boot/efi vfat umask=0077,shortname=winnt 0 2
/dev/disk/by-partlabel/home /home xfs defaults,x-systemd.device-timeout=0 0 0
/dev/disk/by-partlabel/var /var xfs defaults,x-systemd.device-timeout=0 0 0
/dev/disk/by-partlabel/log /var/log xfs defaults,x-systemd.device-timeout=0 0 0
/dev/disk/by-partlabel/audit /var/log/audit xfs defaults,x-systemd.device-timeout=0 0 0
